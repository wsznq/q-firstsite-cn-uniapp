.main-input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: left;
  color: black;
  font-size: 16px;
  padding-right: 6px;
  margin-left: 10px;
}
.icon .mix-list-cell.b-b:after {
  left: 45px;
}
.placeholder-class {
  color: black;
  opacity: 0.5;
}
.mix-list-cell {
  border-radius: 16rpx;
  border: 1px solid #F1F1F1;
  margin-top: 1px;
  font-size: 32rpx;
  background: white;
  text-align: left;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: 32rpx;
  padding: 24rpx 32rpx;
  position: relative;
}
.mix-list-cell.cell-hover {
    background: transparent;
}
.mix-list-cell.b-b:after {
    left: 16px;
}
.mix-list-cell .cell-icon {
    -webkit-align-self: center;
            align-self: center;
    width: 28px;
    max-height: 30px;
    font-size: 18px;
}
.mix-list-cell .cell-more {
    -webkit-align-self: center;
            align-self: center;
    font-size: 16px;
    color: #606266;
    margin-left: 10px;
}
.mix-list-cell .cell-tit {
    width: 80px;
    font-size: 16px;
    color: black;
}
.mix-list-cell .cell-tip {
    font-size: 14px;
    color: black;
}
.items {
  position: absolute;
  height: 48px;
  width: 100%;
  background: #FFFFFF;
  /*opacity:0.05;*/
}
.main-list {
  opacity: 0.8;
  z-index: 88;
  background: white;
  border: 1px solid white;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 18px;
  /* Input 高度 */
  color: #333333;
  padding: 16px;
  margin-top: 12px;
  margin-bottom: 12px;
}
.img {
  width: 16px;
  height: 16px;
  font-size: 16px;
}
.vercode {
  color: #FF6DB2;
  font-size: 14px;
}
.vercode-run {
  color: black !important;
}
.oBorder {
  border-radius: 2.5rem;
  box-shadow: 0 0 30px 0 rgba(43, 86, 112, 0.1);
}

