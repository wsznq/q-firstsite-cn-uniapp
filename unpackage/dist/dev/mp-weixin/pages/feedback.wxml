<view class="page"><view class="feedback-title"><text>问题和意见</text><text data-event-opts="{{[['tap',[['chooseMsg',['$event']]]]]}}" class="feedback-quick" bindtap="__e">快速键入</text></view><view class="feedback-body"><textarea class="feedback-textare" placeholder="请详细描述你的问题和意见..." data-event-opts="{{[['input',[['__set_model',['$0','content','$event',[]],['sendDate']]]]]}}" value="{{sendDate.content}}" bindinput="__e"></textarea></view><view class="feedback-title"><text>QQ/邮箱</text></view><view class="feedback-body"><input class="feedback-input" placeholder="方便我们联系你 " data-event-opts="{{[['input',[['__set_model',['$0','contact','$event',[]],['sendDate']]]]]}}" value="{{sendDate.contact}}" bindinput="__e"/></view><view class="feedback-title feedback-star-view"><text>应用评分</text><view class="feedback-star-view"><block wx:for="{{stars}}" wx:for-item="value" wx:for-index="key" wx:key="key"><text data-event-opts="{{[['tap',[['chooseStar',['$0'],[[['stars','',key]]]]]]]}}" class="{{['feedback-star',key<sendDate.score?'active':'']}}" bindtap="__e"></text></block></view></view><button class="feedback-submit" style="background:rgb(255, 109, 178);" type="primary" data-event-opts="{{[['tap',[['send',['$event']]]]]}}" bindtap="__e">提交</button></view>